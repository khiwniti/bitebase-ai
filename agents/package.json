    "@langchain/openai": "^0.6.13",
    "better-sqlite3": "^11.10.0",
    "canvas": "^3.2.0",
    "chart.js": "^4.5.0",
    "dotenv": "^17.2.2",
    "langsmith": "^0.3.69",
    "playwright": "^1.55.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
