    "start": "node dist/index.js",
    "invoke": "ts-node --esm src/invoke.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src"
  },
  "keywords": [
    "bitebase",
    "ai",
    "agents",
    "langgraph",
    "restaurant",
    "market-research"
  ],
  "author": "BiteBase AI Team",
  "license": "MIT",
  "dependencies": {
    "@executeautomation/playwright-mcp-server": "^1.0.6",
    "@langchain/community": "^0.3.56",
