{
  "name": "bitebase-backend",
  "version": "1.0.0",
  "description": "BiteBase AI Backend - Express.js API server with database integration",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec tsx src/websocket-server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src"
  },
  "keywords": [
    "bitebase",
    "ai",
    "restaurant",
    "market-research",
    "express",
    "api"
  ],
  "author": "BiteBase AI Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  }
}
