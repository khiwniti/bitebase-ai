    "dev": "concurrently \"bun dev:ui\" \"bun dev:agent\" \"bun dev:copilot\" --names ui,agent,copilot --prefix-colors blue,green,yellow --kill-others",
    "dev:studio": "concurrently \"bun dev:ui\" \"bun dev:agent:studio\" --names ui,agent --prefix-colors blue,green --kill-others",
    "dev:agent": "cd agent && npx @langchain/langgraph-cli dev --port 8123 --no-browser",
    "dev:agent:studio": "cd agent && npx @langchain/langgraph-cli dev --port 8123",
    "dev:ui": "next dev",
    "dev:fast": "next dev --turbo",
    "dev:copilot": "npx copilot-api@latest start --github-token $GITHUB_TOKEN --port 4141",
